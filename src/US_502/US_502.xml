<?xml version="1.0" encoding="UTF-8"?>

<suite name="User Login Test">

    <test name="Chrome">
        <parameter name="browserType" value="chrome"/>

        <groups>
            <run>
                <include name="LoginTest"/>
                <include name="Smoke"/>
            </run>
        </groups>

        <classes>
            <class name="US_502.UserLoginTest"/>
        </classes>
    </test>

    <test name="Edge">
        <parameter name="browserType" value="edge"/>

        <groups>
            <run>
                <include name="LoginTest"/>
                <include name="Smoke"/>
            </run>
        </groups>

        <classes>
            <class name="US_502.UserLoginTest"/>
        </classes>
    </test>
</suite>