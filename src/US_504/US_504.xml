    </test>

    <test name="Firefox">
        <parameter name="browserType" value="firefox"/>
        <groups>
            <run>
                <include name="UITesting"/>
                <include name="TAB Menu"/>
            </run>
        </groups>
        <classes>
            <class name ="US_504.TabMenuControlTest"/>
        </classes>
    </test>
